<% include navigation %>
<!DOCTYPE html>
<html>
<head>
<style>
.header {
    padding: 1px;
    text-align: center;
    background: grey;
    color: black;
    font-size: 1px;
}
table {
  font-family: arial, sans-serif;
  border-collapse: collapse;
  width: 100%;
}

td, th {
  border: 1px solid #dddddd;
  text-align: left;
  padding: 8px;
}

tr:nth-child(even) {
  background-color: #dddddd;
}
</style>
</head>
<body>
<div class = header>
<h2 align = "center">Data Entries</h2>
</div>

<% if (results != 0) { %>
<table>
  <tr>
    <th>Team 1 Name</th>
    <th>Team 2 Name</th>
    <th>Alliance</th>
    <th>Match</th>
    <th>Field</th>
    <th>Date Created</th>
    <th>Author</th>
    <th>Actions</th>
  </tr>

  <% for (var i = 0; i < results.length; i++ ) { %>
    <tr>
    <td><%= results[i]['team1_name']%></td>
    <td><%= results[i]['team2_name'] %></td>
    <td><%= results[i]['alliance'] %></td>
    <td><%= results[i]['match_num'] %></td>
    <td><%= results[i]['field'] %></td>
    <td><%= results[i]['create_time'] %></td>
    <td><%= results[i]['Author']%> </td>
    <td><a class="mdl-navigation__link" href="http:/dataView/<%=results[i]['id']%>"><i class="mdl-color-text--black material-icons" role="presentation">remove_red_eye</i></a><a class="mdl-navigation__link" href="http:/dataView1"><i class="mdl-color-text--black material-icons" role="presentation">edit</i></a></td>
  </tr>
   <% } %>
 
 
</table>
<% } %>
</body>
</html>
