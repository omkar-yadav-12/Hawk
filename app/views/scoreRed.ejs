</html>
<% include navigation %>
<style>
    .header {
        padding: 1px;
        text-align: center;
        background: grey;
        color: Red;
        font-size: 1px;
    }
</style>

<head>

<body>

    <link rel="stylesheet" href="https://storage.googleapis.com/code.getmdl.io/1.0.6/material.indigo-pink.min.css">
    <link rel="stylesheet" href="https://fonts.googleapis.com/icon?family=Material+Icons">
    <link rel="stylesheet" href="/css/score.css">
    <script defer src="https://code.getmdl.io/1.3.0/material.min.js"></script>
    </head>
    <title>Red Alliance</title>
    <div class="header">
        <h1>Red Alliance</h1>
    </div>
    <form method="POST" action="/score=?">
        <div class=container>
            <div class="mdl-textfield mdl-js-textfield mdl-textfield--floating-label mdl-cell mdl-cell--2-col">
                <label class="mdl-textfield__label mdl-color-text--grey" for="team_one">Team 1 Number</label>
                <input class="mdl-textfield__input" type="text" id="team_one" name="team_one" required />
                <span class="mdl-textfield__error"></span>
            </div>
            <div class="mdl-textfield mdl-js-textfield mdl-textfield--floating-label mdl-cell mdl-cell--2-col">
                <label class="mdl-textfield__label mdl-color-text--grey" for="team_two">Team 2 Number</label>
                <input class="mdl-textfield__input" type="text" id="team_two" name="team_two" required />
                <span class="mdl-textfield__error"></span>
            </div>
            <div class="mdl-textfield mdl-js-textfield mdl-textfield--floating-label mdl-cell mdl-cell--2-col">
                    <label class="mdl-textfield__label mdl-color-text--grey" for="event_name">Event Name</label>
                    <input class="mdl-textfield__input" type="text" id="event_name" name="event_name" required />
                    <span class="mdl-textfield__error"></span>
                </div>
        </div>
        <div class=container>
            <div class="mdl-textfield mdl-js-textfield mdl-textfield--floating-label mdl-cell mdl-cell--1-col">
                <label class="mdl-textfield__label mdl-color-text--grey" for="match">Match</label>
                <input class="mdl-textfield__input" type="text" id="match" name="match" pattern="^[0-9]{1,3}"
                    required />
                <span class="mdl-textfield__error"></span>
            </div>
            <div class="mdl-textfield mdl-js-textfield mdl-textfield--floating-label mdl-cell mdl-cell--1-col">
                <label class="mdl-textfield__label mdl-color-text--grey" for="field">Field</label>
                <input class="mdl-textfield__input" type="text" id="field" name="field" pattern="^[0-9]{1,3}"
                    required />
                <span class="mdl-textfield__error"></span>
            </div>
        </div>
        <table class="mdl-data-table mdl-js-data-table mdl-shadow--2dp" id="Autonomous">
            <span align="center" class="mdl-layout-title">Autonomous Period</span>
            <thead>
                <tr>
                    <th class="">Type</th>
                    <th>1st</th>
                    <th>2nd</th>
                    <th>3rd</th>
                    <th>4th</th>
                    <th>5th</th>
                    <th>6th</th>
                </tr>
            </thead>
            <tbody>

                <tr>
                    <td class="">Skystone</td>
                    <% for (var i = 1; i <= 6; i++) { %>
                    <td><input type="checkbox" id="skystone<%=i%>" name="skystone<%=i%>" /></td>
                    <% } %>
                </tr>
                <tr>
                    <td class="">Stone</td>
                    <% for (var i = 1; i <= 6; i++) { %>
                    <td><input type="checkbox" name="stone<%=i%>" id="stone<%=i%>" /></td>
                    <% } %>
                </tr>
                <tr>
                    <td class="">None</td>
                    <% for (var i = 1; i <= 6; i++) { %>
                    <td><input type="checkbox" name="none<%=i%>" id="none<%=i%>" /></td>
                    <% } %>
                </tr>

            </tbody>
        </table>
        <table class="mdl-data-table mdl-js-data-table mdl-shadow--2dp" id="Reposition">
            <td class="">First Returned Skystone</td>
            <td><input id="firstRet" type="checkbox" name="FRS" id = "FRS"/></td>
            <td class="">Foundation Repositioned</td>
            <td><input id="firstRep" type="checkbox" name="FS" id = "FS"/></td>
            <td class="">Robot 1 Navigated</td>
            <td><input id="R1N" type="checkbox" name="R1N" id = "R1N" /></td>
            <td class="">Robot 2 Navigated</td>
            <td><input type="checkbox" name="R2N" id = "R2N" /></td>
        </table>
        <div class=container>
            <div class="mdl-textfield mdl-js-textfield mdl-textfield--floating-label mdl-cell mdl-cell--1-col">
                <label class="mdl-textfield__label mdl-color-text--grey" for="ReturnedAuto">Returned</label>
                <input class="mdl-textfield__input" type="text" id="ReturnedAuto" name="ReturnedAuto"
                    pattern="^[0-9]{1,3}" required />
                <span class="mdl-textfield__error"></span>
            </div>
            <div class="mdl-textfield mdl-js-textfield mdl-textfield--floating-label mdl-cell mdl-cell--1-col">
                <label class="mdl-textfield__label mdl-color-text--grey" for="PlacedAuto">Placed</label>
                <input class="mdl-textfield__input" type="text" id="PlacedAuto" name="PlacedAuto" pattern="^[0-9]{1,3}"
                    required />
                <span class="mdl-textfield__error"></span>
            </div>
        </div>

        <span class="mdl-layout-title" align="center">Driver-Controlled Period</span>
        <div class=container>
            <div class="mdl-textfield mdl-js-textfield mdl-textfield--floating-label mdl-cell mdl-cell--1-col">
                <label class="mdl-textfield__label mdl-color-text--grey" for="delivered_number">Delivered</label>
                <input class="mdl-textfield__input" type="text" id="delivered_number" name="delivered_number"
                    pattern="^[0-9]{1,3}" required />
                <span class="mdl-textfield__error"></span>
            </div>
            <div class="mdl-textfield mdl-js-textfield mdl-textfield--floating-label mdl-cell mdl-cell--1-col">
                <label class="mdl-textfield__label mdl-color-text--grey" for="tallest_skyscraper">Tallest
                    Skyscraper</label>
                <input class="mdl-textfield__input" type="text" id="tallest_skyscraper" name="tallest_skyscraper"
                    pattern="^[0-9]{1,3}" required />
                <span class="mdl-textfield__error"></span>
            </div>

            <div class="mdl-textfield mdl-js-textfield mdl-textfield--floating-label mdl-cell mdl-cell--1-col">
                <label class="mdl-textfield__label mdl-color-text--grey" for="returned_name">Returned</label>
                <input class="mdl-textfield__input" type="text" id="returned_name" name="returned_name"
                    pattern="^[0-9]{1,3}" required />
                <span class="mdl-textfield__error"></span>
            </div>

            <div class="mdl-textfield mdl-js-textfield mdl-textfield--floating-label mdl-cell mdl-cell--1-col">
                <label class="mdl-textfield__label mdl-color-text--grey" for="placed">Placed</label>
                <input class="mdl-textfield__input" type="text" id="placed" name="placed" pattern="^[0-9]{1,3}"
                    required />
                <span class="mdl-textfield__error"></span>
            </div>
        </div>

        <table class="mdl-data-table mdl-js-data-table mdl-shadow--2dp" id="Found">
            <span align=center class="mdl-layout-title">End Game Period</span>
            <td class="">Foundation Moved</td>
            <td><input type="checkbox" name="found" id="found" /></td>
        </table>
        <table class="mdl-data-table mdl-js-data-table mdl-shadow--2dp" id="Robots">
            <thead>
                <tr>
                    <th class=""></th>
                    <th>Robot One</th>
                    <th>Robot Two</th>
                </tr>
            </thead>
            <tr>
                <td class="">Capstone</td>
                <td><input type="checkbox" name="Capstone1" id = "Capstone1"/></td>
                <td><input type="checkbox" name="Capstone2" id = "Capstone2"/></td>
            </tr>
            <tr>
                <td class="">Parked</td>
                <td><input type="checkbox" name="Parked1" id = "Parked1" /></td>
                <td><input type="checkbox" name="Parked2" id = "Parked2" /></td>
            </tr>
        </table>
        <div class=container>
            <div class="mdl-textfield mdl-js-textfield mdl-textfield--floating-label mdl-cell mdl-cell--1-col">
                <label class="mdl-textfield__label mdl-color-text--grey" for="robot1_level">Robot 1 Level</label>
                <input class="mdl-textfield__input" type="text" id="robot1_level" name="robot1_level"
                    pattern="^[0-9]{1,3}" required />
                <span class="mdl-textfield__error"></span>
            </div>
            <div class="mdl-textfield mdl-js-textfield mdl-textfield--floating-label mdl-cell mdl-cell--1-col">
                <label class="mdl-textfield__label mdl-color-text--grey" for="robot2_level">Robot 2 Level</label>
                <input class="mdl-textfield__input" type="text" id="robot2_level" name="robot2_level"
                    pattern="^[0-9]{1,3}" required />
                <span class="mdl-textfield__error"></span>
            </div>
        </div>

        <span class="mdl-layout-title" align="center">Penalties</span>
        <div class=container>
            <div class="mdl-textfield mdl-js-textfield mdl-textfield--floating-label mdl-cell mdl-cell--1-col">
                <label class="mdl-textfield__label mdl-color-text--grey" for="minor"># Minor</label>
                <input class="mdl-textfield__input" type="text" id="minor" name="minor" pattern="^[0-9]{1,3}"
                    required />
                <span class="mdl-textfield__error"></span>
            </div>
            <div class="mdl-textfield mdl-js-textfield mdl-textfield--floating-label mdl-cell mdl-cell--1-col">
                <label class="mdl-textfield__label mdl-color-text--grey" for="major"># Major</label>
                <input class="mdl-textfield__input" type="text" id="major" name="major" pattern="^[0-9]{1,3}"
                    required />
                <span class="mdl-textfield__error"></span>
            </div>
        </div>
        <table class="mdl-data-table mdl-js-data-table mdl-shadow--2dp" id="Cards">
            <thead>
                <tr>
                    <th class=""></th>
                    <th>No Show</th>
                    <th>Yellow Card</th>
                    <th>Red Card</th>
                </tr>
            </thead>
            <tr>
                <td class="">Team 1</td>
                <% for (var i = 0; i <= 2; i++) { %>
                <td><input type="checkbox" id = "team1_<%=i%>" name="team1_<%=i%>" /></td>
                <% } %>
            </tr>
            <tr>
                <td class="">Team 2</td>
                <% for (var i = 0; i <= 2; i++) { %>
                <td><input type="checkbox" id = "team2_<%=i%>" name="team2_<%=i%>" /></td>
                <% } %>
            </tr>
        </table>
        <div class="mdl-textfield mdl-js-textfield mdl-textfield--floating-label mdl-cell mdl-cell--2-col">
                <label class="mdl-textfield__label mdl-color-text--grey" for="alliance"></label>
                <input class="mdl-textfield__input" type="text" id="alliance" name="alliance" value = "Red" hidden />
                <span class="mdl-textfield__error"></span>
        </div>
        <a href="/score" id="backButton" class="mdl-button mdl-js-button mdl-button--fab mdl-button--mini-fab mdl-color-text--green">
            <i class="material-icons">arrow_back</i>
        </a>
        <input id="resetButton" type="reset" onlclick = toatal()
            class="mdl-button mdl-js-button mdl-button--raised mdl-js-ripple-effect mdl-color--black mdl-color-text--green"
            value="Reset">
        <input id="submitButton" type="submit" 
            class="mdl-button mdl-js-button mdl-button--raised mdl-js-ripple-effect mdl-color--black mdl-color-text--green"
            value="Submit">
        <script>
            function total() {
                tallest = document.getElementById("tallest_skyscraper") * 2
                alert(tallest)
            }
        </script>
    </form>
    </html>
