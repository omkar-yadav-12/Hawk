<% include navigation %>

<style>
</style>
<div class=header>
    <h2>Team Data Entries</h2>
</div>
<title>Team Data</title>
<body>
    <link rel="stylesheet" href="/css/scoringButtons.css">
    <link rel="stylesheet" href="/css/table.css">

    <table>
        <tr>
            <th>Team Number</th>
            <th>Name</th>
            <th>School</th>
            <th>Location</th>
            <th>League</th>
            <th>Actions</th>
        </tr>

        <% for (var i = 0; i < results.length; i++ ) { %>
        <tr>
            <td><%= results[i]['team_number']%></td>
            <td><%= results[i]['name'] %></td>
            <td><%= results[i]['school'] %></td>
            <td><%= results[i]['location'] %></td>
            <td><%= results[i]['league'] %></td>
            <td><a class="mdl-navigation__link" href="http:/teamEdit/<%=results[i]['idteam']%>"><i
                        class="mdl-color-text--black material-icons" role="presentation">edit</i></a><a
                    class="mdl-navigation__link" href="http:/teamEdit/delete/<%=results[i]['idteam']%>"><i
                        onclick="return confirm('Are you sure you want to delete this entry?');"
                        class="mdl-color-text--black material-icons" role="presentation">delete</i></a></td>
        </tr>
        <% } %>
    </table>



    <a href="/data" id="backButton"
        class="mdl-button mdl-js-button mdl-button--fab mdl-button--mini-fab mdl-color-text--green">
        <i class="material-icons">arrow_back</i>
    </a>
    <a href="/newTeam" id="new"
        class="mdl-button mdl-js-button mdl-button--fab mdl-button--mini-fab mdl-color-text--green">
        <i class="material-icons">add</i>
    </a>
    <a href="/downloadTournament" id="exportButton"
        class="mdl-button mdl-js-button mdl-button--raised mdl-js-ripple-effect mdl-color--black mdl-color-text--green">Download</a>