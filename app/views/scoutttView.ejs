<%include navigation%>
<!DOCTYPE html>

<html>
<style>
</style>
<head>
    <link rel="stylesheet" href="https://fonts.googleapis.com/icon?family=Material+Icons">
    <link rel="stylesheet" href="https://code.getmdl.io/1.3.0/material.indigo-pink.min.css">
    <link rel="stylesheet" href="/css/scoringButtons.css">
    <script defer src="https://code.getmdl.io/1.3.0/material.min.js"></script>
    <script defer src="/js/scout.js"></script>
    
    <link rel='stylesheet' href='/css/scout.css' />
    <link rel='stylesheet' href='/css/score.css' />
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Scout</title>
</head>

<button class="tablink" onclick="openPage('Home', this, 'yellow')" id="defaultOpen">Info</button>
<button class="tablink" onclick="openPage('News', this, 'yellow')">Autonomous</button>
<button class="tablink" onclick="openPage('Contact', this, 'yellow')">Teleop</button>
<button class="tablink" onclick="openPage('About', this, 'yellow')">End Game</button>

<div id="Home" class="tabcontent">
    <div class=container>
        <div class="mdl-textfield mdl-js-textfield mdl-textfield--floating-label mdl-cell mdl-cell--2-col mdl-color-text--yellow">
            <label class="mdl-textfield__label mdl-color-text--grey" for="team_num">Team Number</label>
            <input class="mdl-textfield__input mdl-color-text--yellow" type="text" id="team_num"
                value="<%=results[0]['team_number']%>" name="team_num" readonly />
            <span class="mdl-textfield__error"></span>
        </div>
        <div class="mdl-textfield mdl-js-textfield mdl-textfield--floating-label mdl-cell mdl-cell--2-col mdl-color-text--yellow">
            <label class="mdl-textfield__label mdl-color-text--grey" for="event_name">Event Name</label>
            <input class="mdl-textfield__input mdl-color-text--yellow" type="text" id="event_name"
                value="<%=results[0]['event_name']%>" name="event_name" required />
            <span class="mdl-textfield__error"></span>
        </div>
    </div>
</div>
<a href="/scoutingData" id="backButton"
    class="mdl-button mdl-js-button mdl-button--fab mdl-button--mini-fab mdl-color-text--yellow">
    <i class="material-icons">arrow_back</i>
</a>

</div>

<div id="News" class="tabcontent">
    <table class="mdl-data-table mdl-js-data-table mdl-shadow--2dp mdl-color--black">
        <td class="">Move Foundation</td>
        <% if (results[0]['move_auto'] == 1 ) { %>
        <td><input id="firstRet" type="checkbox" onclick="return false;" readonly checked name="MF" id="MF" /></td>
        <% } else { %>
        <td><input id="firstRet" type="checkbox" onclick="return false;" readonly name="MF" id="MF" /></td>
        <% } %>

        <td class="">Sense Skystones</td>
        <% if (results[0]['sense_auto'] == 1 ) { %>
        <td><input id="firstRet" type="checkbox" onclick="return false;" readonly checked name="SS" id="SS" /></td>
        <% } else { %>
        <td><input id="firstRet" type="checkbox" onclick="return false;" readonly name="MF" id="MF" /></td>
        <% } %>

        <td class="">Park Over the Build Zone</td>
        <% if (results[0]['over_auto'] == 1 ) { %>
        <td><input id="firstRet" type="checkbox" onclick="return false;" readonly checked name="PDZ" id="PDZ" /></td>
        <% } else { %>
        <td><input id="firstRet" type="checkbox" onclick="return false;" readonly name="PDZ" id="PDZ" /></td>
        <% } %>
        <td class="">Collect Stones</td>
        <% if (results[0]['collect_auto'] == 1 ) { %>
