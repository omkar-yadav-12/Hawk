            <!-- <label class="mdl-textfield__label  mdl-color-text--yellow" id=expand_label2 for="expand2">Sensing
                    Description</label> -->
        </div>
        <div class="mdl-textfield mdl-js-textfield">
            <textarea class="mdl-textfield__input  mdl-color-text--yellow" type="text" readonly rows="10" name=expand3
                id="expand3"><%=results[0]['park_auto_d']%></textarea>
            <!-- <label class="mdl-textfield__label  mdl-color-text--yellow" id=expand_label3 for="expand3">Parking
                    Description</label> -->
        </div>
        <div class="mdl-textfield mdl-js-textfield">
            <textarea class="mdl-textfield__input  mdl-color-text--yellow" type="text" readonly rows="10" name=expand4
                id="expand4"><%=results[0]['stone_auto_d']%></textarea>
