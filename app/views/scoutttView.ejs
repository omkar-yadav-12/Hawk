        <% } else { %>
        <td><input id="firstRet" type="checkbox" onclick="return false;" readonly name="CS" id="CS" /></td>
        <% } %>
        <td class="">Place Stones</td>
        <% if (results[0]['place_auto'] == 1 ) { %>
        <td><input id="firstRet" type="checkbox" onclick="return false;" readonly checked name="FS" id="FS" /></td>
        <% } else { %>
        <td><input id="firstRet" type="checkbox" onclick="return false;" readonly name="FS" id="FS" /></td>
        <% } %>

    </table>
    <div class=container1>
        <div class="mdl-textfield mdl-js-textfield">
            <textarea class="mdl-textfield__input  mdl-color-text--yellow" readonly type="text" rows="10" name=expand1
                id="expand1"><%=results[0]['found_auto_d']%></textarea>
            <!-- <label class="mdl-textfield__label  mdl-color-text--yellow" id=expand_label1  for="expand1">Foundation
                    Description</label> -->
        </div>
        <div class="mdl-textfield mdl-js-textfield">
            <textarea class="mdl-textfield__input  mdl-color-text--yellow" type="text" readonly rows="10" name=expand2
