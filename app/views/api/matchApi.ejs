<%- include ('../navigation.ejs')%>

<head>
    <h1> <span class="red">Matches</pan>
    </h1>
    <table class=container>
        <thead>
            <tr>
                <th>
                    <h1>Name</h1>
                </th>
                <th>
                    <h1>Red 1</h1>
                </th>
                <th>
                    <h1>Red 2</h1>
                </th>
                <th>
                    <h1>Blue 1</h1>
                </th>
                <th>
                    <h1>Blue 2</h1>
                </th>
                <th>
                    <h1>Auto Score</h1>
                </th>
                <th>
                    <h1>Teleop Score</h1>
                </th>
                <th>
                    <h1>End Score</h1>
                </th>
                <th>
                    <h1>Total Score</h1>
                </th>
                <th>
                    <h1>View</h1>
                </th>

            </tr>

        </thead>
        <tbody>
            <% for (let i = 0; i < array.length; i++ ) { %>
            <tr>
                <td><%= array[i].match_name%></td>
                <td><%= array[i]['participants'][0].team_key %></td>
                <td><%= array[i]['participants'][1].team_key %></td>
                <td><%= array[i]['participants'][2].team_key %></td>
                <td><%= array[i]['participants'][3].team_key %></td>
                <td><%= array[i].red_auto_score%> - <%=array[i].blue_auto_score%></td>
                <td><%= array[i].red_tele_score%> - <%=array[i].blue_tele_score%></td>
                <td><%= array[i].red_end_score%> - <%=array[i].blue_end_score%></td>
                <td><%= array[i].red_score%> - <%=array[i].blue_score%></td>
                <td><a class="mdl-navigation__link"
                        href="/api/event/<%=array[i].event_key%>/<%=array[i].match_key%>"><i
                            class="mdl-color-text--black material-icons" role="presentation">remove_red_eye</i></a></td>
            </tr>
            <% } %>
        </tbody>
    </table>
    <a href="/api/event/<%=array[0].event_key%>/rankings" id="exportButton"
    class="mdl-button mdl-js-button mdl-button--raised mdl-js-ripple-effect mdl-color--black mdl-color-text--yellow">Rankings</a>
    