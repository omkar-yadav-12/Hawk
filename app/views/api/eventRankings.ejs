        <td>(<%= json[i]['team'].team_number%>) <%=json[i]['team'].team_name_short%> <%=json[i]['team'].city%> <%= json[i]['team'].state_prov %></td>
        <td><%= json[i].wins%> - <%= json[i].losses%> - <%= json[i].ties%> </td>
        <td><%= json[i].opr%></td>
