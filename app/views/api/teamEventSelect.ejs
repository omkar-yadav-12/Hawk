<%- include ../navigation%>

<h1>Tournaments</h1>

<table class=container>
    <thead>
        <tr>
            <th>
                <h1>Tournament Name</h1>
            </th>
            <th>
                <h1>Venue</h1>
            </th>
            <th>
                <h1>City</h1>
            </th>
            <th>
                <h1>State</h1>
            </th>
            <th>
                <h1>Date</h1>
            </th>
            <th>
                <h1>View</h1>
            </th>
            <!-- <th>
                <h1>W-L</h1>
            </th>
            <th>
                <h1>Select</h1>
            </th>  -->
        </tr>
    </thead>
    <tbody>
        <% for (let i = 0; i < events.length; i++ ) { %>
        <tr>
            <td><%= events[i]._eventName%></td>
            <td><%= events[i]._venue%></td>
            <td><%= events[i]._city %></td>
            <td><%= events[i]._stateProv %></td>
            <td><%= events[i]._startDate%></td>
            <td><a class="mdl-navigation__link" href="/api/event/<%=events[i]._eventKey%>/matches"><i
                        class="mdl-color-text--black material-icons" role="presentation">remove_red_eye</i></a>
                <a class="mdl-navigation__link" href="/eventSimulation/<%=events[i]._eventKey%>"><i
                        class="mdl-color-text--black material-icons" role="presentation">event</i></a></td>
        </tr>
        <% } %>
    </tbody>
</table>